    <!--防止白色启动背景-->
    <style name="StartBackground">
        <item name="colorPrimary">?android:attr/colorPrimary</item>
        <item name="android:windowBackground">@color/start_background</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
    <!-- Base application theme. -->
    <style name="BaseLight" parent="Theme.AppCompat.Light.DarkActionBar" >
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:popupTheme">@style/ThemeOverlay.AppCompat.Light</item>
    </style>

    <style name="BaseDark" parent="Theme.Design.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <!--A屏黑-->
    <style name="ABlack" parent="Theme.Design.NoActionBar">
        <!--<item name="colorPrimaryDark">@color/md_black_1000</item>-->
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:navigationBarColor">@color/md_black_1000</item>
        <item name="android:colorBackground">@color/md_black_1000</item>
        <item name="android:actionBarStyle">@style/ActionBar_Dark</item>
        <item name="android:popupTheme">@style/ThemeOverlay.AppCompat.Dark</item>
    </style>

    <style name="ActionBar_Dark">
        <item name="android:background">#000</item>
        <item name="android:elevation">10dp</item>
    </style>
